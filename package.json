{
  "name": "flipid",
  "version": "0.4.2",
  "description": "Simple and Reversible ID Transformation",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "build": "bun run build:esm && bun run build:cjs && tsconfig-to-dual-package",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "prepublishOnly": "bun run build",
    "example": "tsx -e 'import {runExamples} from \"./src/example.ts\"; runExamples();'",
    "test": "vitest --coverage",
    "test:watch": "vitest watch",
    "tsx": "tsx"
  },
  "dependencies": {
    "bufferbase": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.0.0",
    "tsconfig-to-dual-package": "^1.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^2.1.8"
  },
  "files": [
    "dist"
  ],
  "author": "misebox",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/misebox/flipid/issues"
  },
  "homepage": "https://github.com/misebox/flipid#readme"
}
